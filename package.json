{
  "name": "@sendgrid/ui-components-base",
  "version": "0.14.6",
  "description": "Resuable UI components for Sendgrid's applications.",
  "scripts": {
    "start": "yarn run storybook",
    "declaresass": "webpack",
    "build": "webpack && tsc --newline lf && bash copyStyles.sh",
    "lerna": "lerna publish",
    "lint": "tslint --project ./tsconfig.json",
    "lint-fix": "tslint --project ./tsconfig.json --fix",
    "test": "jest --coverage",
    "update-snapshot": "jest -u",
    "ci-test": "yarn lint && yarn test --ci",
    "storybook": "start-storybook -s ./ -p 6006",
    "build-storybook": "build-storybook -o ./docs",
    "prepublishOnly": "yarn build",
    "precommit": "yarn lint-fix && lint-staged",
    "snapshot": "yarn test -u",
    "lint-snapshot": "yarn lint-fix && yarn snapshot",
    "image-snapshots": "./imageSnapshot.sh",
    "ci-only-image-snapshots": "yarn build-storybook && jest --projects=./test_image --forceExit -i"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": "https://github.com/sendgrid/ui-components.git",
  "author": "SendGrid <careers@sendgrid.com>",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/style-guide-grid": "^0.3.0",
    "@types/classnames": "^2.2.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.3",
    "@types/storybook__addon-actions": "^3.0.1",
    "@types/storybook__react": "^3.0.5",
    "classnames": "^2.2.5",
    "lerna": "^2.9.0",
    "lodash": "^4.17.4",
    "raf": "^3.4.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "devDependencies": {
    "@storybook/addon-storyshots": "^3.4.5",
    "@storybook/addon-storysource": "^3.4.5",
    "@storybook/react": "^3.4.5",
    "@types/acorn": "^4.0.3",
    "@types/chokidar": "^1.7.4",
    "@types/enzyme": "^3.1.5",
    "@types/jest": "^21.1.8",
    "@types/lodash": "^4.14.92",
    "@types/react-test-renderer": "^16.0.0",
    "awesome-typescript-loader": "^3.4.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "change-case": "^3.0.2",
    "css-loader": "^0.28.11",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "glob": "^7.1.2",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.1.0",
    "jest-cli": "^22.4.3",
    "lint-staged": "^7.1.3",
    "magic-string": "^0.22.4",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.4",
    "prettier": "1.13.4",
    "react-animate-height": "^2.0.1",
    "react-test-renderer": "^16.3.2",
    "regenerator-runtime": "^0.11.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.55.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-postcss": "^1.6.1",
    "rollup-plugin-typescript2": "^0.13.0",
    "sass-loader": "^6.0.6",
    "source-map": "^0.7.0",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.3.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-react": "^3.2.0",
    "typescript": "2.8.1",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "^3.11.0"
  }
}
